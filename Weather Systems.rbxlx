	local Child = game.ServerStorage.TTPPluginAdmins:GetChildren()
	for i = 1, 24 do
		Admin.AdminButtons["Button" .. i].TextColor3 = Color3.new(1, 1, 1)
		Admin.AdminButtons["Button" .. i].BackgroundColor3 = Color3.new(1, 1, 1)
		if Child[i] ~= nil then
