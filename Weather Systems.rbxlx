		ForecastDay.Temperature.Value = AverageHighTemps[CurrentMonth]
		ForecastDay.Weather.Value = "Sunny"
		ForecastNight.Temperature.Value = AverageLowTemps[CurrentMonth]
		ForecastNight.Weather.Value = "Clear"
	end
