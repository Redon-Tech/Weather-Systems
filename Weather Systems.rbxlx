			end
			local sound = Instance.new("Sound")
			sound.Parent = script
			sound.SoundId = "rbxassetid://" .. Id
			sound.Volume = 1
			local pc = math.random(-2, 2) / 10
			sound.PlaybackSpeed = 1 + pc
			sound:Play()
			wait(20)
			sound:remove()
		end
	end
end)

